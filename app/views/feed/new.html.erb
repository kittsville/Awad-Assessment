<div class="panel panel-default feed-panel">
  <div class="panel-heading">
    <h3 class="panel-title">Add a Feed</h3>
  </div>
  <div class="panel-body">
    <%= form_tag("/create_feed", method: "get") do %>
      <div class="field input-group">
        <%= label :feed, :url %><br />
        <%= text_field :feed_url, autofocus: true, class: "form-control", autocomplete: "off" %>
      </div>

      <div class="actions">
        <%= button_tag :validate, class: "btn btn-primary" %>
      </div>

      <div class="actions">
        <%= submit_tag :submit, class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
<script type="text/javascript">
</script>
